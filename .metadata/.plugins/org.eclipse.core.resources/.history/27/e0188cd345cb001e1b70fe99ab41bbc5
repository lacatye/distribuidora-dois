package application;

import entities.InvoicingMensal;

public class Main {

	public static void main(String[] args) {

		InvoicingMensal fat = new InvoicingMensal();

		fat.addInvoicing("SP", 67836.43);
		fat.addInvoicing("RJ", 36678.66);
		fat.addInvoicing("MG", 29229.88);
		fat.addInvoicing("ES", 27165.48);
		fat.addInvoicing("Outros", 19849.53);

		fat.calcPercentual();
	}
}
